{
  "name": "djano-react-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Django react redux ssr boilerplate",
  "main": "index.js",
  "scripts": {
    "development": "NODE_PATH=./src NODE_ENV=development node src/server/server.babel.js",
    "production": "NODE_PATH=./src NODE_ENV=production node src/server/server.babel.js",
    "build": "yarn build:client && yarn build:server",
    "build:server": "NODE_ENV=production webpack --config webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config webpack/client.babel.js",
    "watch": "NODE_ENV=production webpack --config webpack/client.babel.js --watch",
    "mocha": "mocha --require tests/require --recursive --compilers js:babel-core/register src/universal"
  },
  "keywords": [
    "react",
    "redux",
    "hot-module-reloading",
    "server-side-rendering",
    "hmr",
    "ssr",
    "code-splitting",
    "webpack"
  ],
  "author": "Kyle Murphy <ruffers39@gmail.com>",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.0",
    "chai-enzyme": "^0.8.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "9.8.3",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "sass-material-colors": "^0.0.5",
    "sinon": "^3.0.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "compression": "^1.7.0",
    "copy-webpack-plugin": "^4.0.1",
    "express": "^4.15.2",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.5",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-form": "^7.0.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
